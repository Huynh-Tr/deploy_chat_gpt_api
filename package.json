{
  "name": "cloudflare-puter-api-app",
  "version": "1.0.0",
  "description": "Web app for deploying through Cloudflare with Puter API integration and n8n support",
  "main": "index.html",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "node deploy.js",
    "deploy:pages": "node deploy-to-pages.js",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:prod": "wrangler deploy",
    "start": "wrangler dev --local",
    "tail": "wrangler tail",
    "test:pages": "node test-pages-deployment.js",
    "test:webhook": "node test-webhook.js"
  },
  "keywords": [
    "cloudflare",
    "puter",
    "openai",
    "n8n",
    "web-app"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "wrangler": "^3.78.12"
  }
}
